{
  "name": "sybli-backend",
  "version": "1.0.0",
  "description": "Backend API for Sybli - Biometric NFT Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "docker:build": "docker build -t sybli-backend .",
    "docker:run": "docker run -p 3000:3000 sybli-backend",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "nodejs",
    "express",
    "api",
    "blockchain",
    "nft",
    "biometric",
    "web3"
  ],
  "author": "Sybli Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.7",
    "web3": "^4.3.0",
    "sequelize": "^6.35.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "multer": "^1.4.5-lts.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "redis": "^4.6.11",
    "aws-sdk": "^2.1497.0",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "validator": "^13.11.0",
    "crypto-js": "^4.2.0",
    "ethers": "^6.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "sequelize-cli": "^6.35.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "cross-env": "^7.0.3",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/sybli-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/sybli-backend/issues"
  },
  "homepage": "https://github.com/your-org/sybli-backend#readme"
}
